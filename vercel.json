{
  "version": 2,
  "name": "tabor3-donation-widget",
  "builds": [
    { "src": "**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/(embed\\.js|manifest\\.json)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/$1"
    },
    { "src": "/", "dest": "/donation.html" },
    { "src": "/donate", "dest": "/donation.html" }
  ]
}
